mode lib

// Printing functions

single_print := ext_proc(s: Str);
print_flush  := ext_proc();

print := proc(args: ..Str) {
    for val: Str in args {
        single_print(val)
    }
    print_flush()
}

println := proc(args: ..Str) {
    for val: Str in args {
        single_print(val)
    }
    single_print("\n")
    print_flush()
}
