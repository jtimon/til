mode lib

// Printing functions

single_print := ext_proc(s: Str) {}
print_flush  := ext_proc() {}

print := proc(args: ..Str) {
    for print_arg: Str in args {
        single_print(print_arg)
    }
    print_flush()
}

println := proc(args: ..Str) {
    for print_arg: Str in args {
        single_print(print_arg)
    }
    single_print("\n")
    print_flush()
}
