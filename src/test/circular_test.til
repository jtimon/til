mode test

import("test.helpers.circular_a")
import("test.helpers.circular_b")

test_circular : proc() = {
    a := TypeA(val=42)

    b := TypeB(val=100)

    // Test that we can use functions from both modules
    result_a := use_b(b)
    result_b := use_a(a)

    assert_eq(loc(), 100, result_a)
    assert_eq(loc(), 42, result_b)
}

test_circular()
println("Circular import test passed!")
